{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "paths": {
      "@/*": ["./src/*"]
    },
    "baseUrl": "."
  },
  "env": {
    "targets": {
      "node": 16
    }
  },
  "module": {
    "type": "nodenext"
  }
}